{
  "name": "typescript-yarn-project",
  "version": "1.0.0",
  "description": "TypeScript project using yarn for setup-node testing",
  "main": "dist/index.js",
  "packageManager": "yarn@4.4.1",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "npm run build && node dist/test.js",
    "test:direct": "ts-node src/test.ts",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "yarn",
    "testing",
    "setup-node"
  ],
  "author": "setup-node-test",
  "license": "MIT"
}
